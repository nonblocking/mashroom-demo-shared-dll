{
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "flowtype"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "jest": true
    },
    "settings": {
        "react": {
            "version": "16.11"
        }
    },
    "rules": {
        "no-unused-vars": "warn"
    }
}
