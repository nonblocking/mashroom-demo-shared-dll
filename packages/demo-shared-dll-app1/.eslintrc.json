{
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "react",
        "@typescript-eslint/eslint-plugin"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "jest": true
    },
    "settings": {
        "react": {
            "version": "17"
        }
    },
    "rules": {
        "no-unused-vars": "warn",
        "@typescript-eslint/ban-ts-comment": "off"
    }
}
